<!-- END PAGE CONTENT INNER -->
<script type="text/javascript">
    $(document).ready(function () {
        LoadChart();
        setTimeout(function () { searchData(); ViewSource(); }, 300);
        
        //initChart1()
    });
    function getLastWeek(d) {
        var today = new Date();
        var lastWeek = new Date(today.getFullYear(), today.getMonth(), today.getDate() - d);
        var lastWeekMonth = lastWeek.getMonth() + 1;
        var lastWeekDay = lastWeek.getDate();
        var lastWeekYear = lastWeek.getFullYear();

        var lastWeekDisplay = lastWeekMonth + "/" + lastWeekDay + "/" + lastWeekYear;
        var lastWeekDisplayPadded = ("00" + lastWeekMonth.toString()).slice(-2) + "/" + ("00" + lastWeekDay.toString()).slice(-2) + "/" + ("0000" + lastWeekYear.toString()).slice(-4);
        return lastWeekDisplayPadded;
    }
    function ViewSource() {
        var t = $("#slTHNgay").val();
        var datenow = new Date();
        var nDay = datenow.getDate();
        if (nDay < 10) {
            nDay = "0" + nDay;
        }
        var nMonth = datenow.getMonth() + 1;
        if (nMonth < 10) {
            nMonth = "0" + nMonth;
        }
        var nYear = datenow.getFullYear();
        var fullDate = nMonth + "/" + nDay + "/" + nYear;

        var idtab = $("div[class='tab-pane active']").attr('id');
        console.log(idtab);
        if (idtab == undefined) {
            idtab = "tab_1"
        }
        console.log(idtab);

        if (t == "0") {
            $('#datepickerFrom').datepicker("setDate", fullDate);
            $('#datepickerTo').datepicker("setDate", fullDate);
            searchData();
            LoadChart();
            ActTab(idtab);
        }
        if (t == "1") {

            $('#datepickerFrom').datepicker("setDate", "-1d");
            $('#datepickerTo').datepicker("setDate", "-1d");
            searchData();
            LoadChart();
            ActTab(idtab);
        }
        if (t == "2") {
            $('#datepickerFrom').datepicker("setDate", "-2d");
            $('#datepickerTo').datepicker("setDate", "-2d");
            searchData();
            LoadChart();
            ActTab(idtab);
        }
        if (t == "3") {
            $('#datepickerFrom').datepicker("setDate", "-3d");
            $('#datepickerTo').datepicker("setDate", "-3d");
            searchData();
            LoadChart();
            ActTab(idtab);
        }
        if (t == "4") {
            $('#datepickerFrom').datepicker("setDate", "-4d");
            $('#datepickerTo').datepicker("setDate", "-4d");
            searchData();
            LoadChart();
            ActTab(idtab);
        }
        if (t == "5") {
            $('#datepickerFrom').datepicker("setDate", "-5d");
            $('#datepickerTo').datepicker("setDate", "-5d");
            searchData();
            LoadChart();
            ActTab(idtab);
        }
    }
    function ActTab(idtab) {
        $("div[id^='tab']").each(function () {
            if ($(this).attr('id') == idtab) {
                $(this).addClass('active');
                $("a[href='#" + $(this).attr('id')+"']").parent().addClass('active');
            }
            else {
                $(this).removeClass('active');
                $("a[href='#" + $(this).attr('id') + "']").parent().removeClass('active');
            }
        });
    }
    function LoadChart()
    {
        var strServiceID = "";
        var serviceID = $('#slDichVu').val();
        if (serviceID == null || serviceID == '') serviceID = 0;
        else {
            if (serviceID.length > 0) {
                for (var i = 0; i < serviceID.length; i++) {
                    if (i == serviceID.length - 1)
                        strServiceID += serviceID[i];
                    else
                        strServiceID += serviceID[i] + ',';
                }
            }
            serviceID = strServiceID;
        }
                        $.ajax({
                            type: "GET",
                            url: "/Report/NewStatistics_Char",
                            data: {
                                typeShow: $('#slKieuPhanTich').val(),
                                time: $('#slThoiGian').val(),
                                serviceID: serviceID
                            },
                            contentType: "application/json; charset=utf-8",
                            dataType: "html",
                            success: function (data) {

                                if (data != null) {
                                    $('#contentChart').html(data);
                                }
                            },
                            error: function () {
                                bootbox.alert("Hệ thống bận, vui lòng quay lại sau!");
                            }
                        });
    }

    function ExportExcel() {
        var strServiceID = "";
        var slKieuPhanTich = $('#slKieuPhanTich').val();
        var serviceID = $('#slDichVu').val();
        var datepickerFrom = $('#datepickerFrom').val();
        var datepickerTo = $('#datepickerTo').val();
        var chkTH = $('#chkTH').is(":checked");

        if (serviceID == null || serviceID == '') serviceID = 0;
        else {
            if (serviceID.length > 0) {
                for (var i = 0; i < serviceID.length; i++) {
                    if (i == serviceID.length - 1)
                        strServiceID += serviceID[i];
                    else
                        strServiceID += serviceID[i] + ',';
                }
            }
            serviceID = strServiceID;
        }
        var link = "/Report/Statistics_Excel" + "?serviceID=" + serviceID + "&startDate=" + datepickerFrom + "&endDate=" + datepickerTo + "&typeShow=" + slKieuPhanTich + "&chkTH=" + (chkTH == true ? "1" : "0") + "&time=" + $('#slThoiGian').val();
        window.location.href = link;
    }

    function searchData()
    {
        ShowLoading();
        var strServiceID = "";
        var slKieuPhanTich = $('#slKieuPhanTich').val();
        var serviceID = $('#slDichVu').val();
         var datepickerFrom = $('#datepickerFrom').val();
         var datepickerTo = $('#datepickerTo').val();
        var chkTH = $('#chkTH').is(":checked");
        var idtab = $("div[class='tab-pane active']").attr('id');
        console.log(idtab);
        if (idtab == undefined) {
            idtab = "tab_1"
        }
        console.log(idtab);
         if (serviceID == null || serviceID == '') serviceID = 0;
         else {
             if (serviceID.length > 0) {
                 for (var i = 0; i < serviceID.length; i++) {
                     if (i == serviceID.length - 1)
                         strServiceID += serviceID[i];
                     else
                     strServiceID += serviceID[i] + ',';
                 }
             }
             serviceID = strServiceID;
         }
            $.ajax({
                type: "GET",
                url: "/Report/NewStatistics_Part",
                data: {
                    serviceID: serviceID,
                    startDate: datepickerFrom,
                    endDate: datepickerTo,
                    typeShow: slKieuPhanTich,
                    chkTH: chkTH == true ? 1 : 0,
                    time: $('#slThoiGian').val(),
                },
                contentType: "application/json; charset=utf-8",
                dataType: "html",
                success: function (data) {

                    if (data != null) {
                        $('.tab-content').html(data);
                        //fomartDataTable();
                        HideLoading();
                        ActTab(idtab);
                    }
                },
                error: function () {
                    bootbox.alert("Hệ thống bận, vui lòng quay lại sau!");
                }
         });
    }

</script>

            </div>
